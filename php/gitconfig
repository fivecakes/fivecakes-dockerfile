[user]
        email=git@fivecakes.com
        name=hooks
[core]
        ignorecase = false              # 不许忽略文件名大小写
        autocrlf = input                # 换行模式为 input，即提交时转换为LF，检出时不转换
        filemode = false                # 不检查文件权限
        safecrlf = true                 # 拒绝提交包含混合换行符的文件
        editor = vim                    # 默认编辑器